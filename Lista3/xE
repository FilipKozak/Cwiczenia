#include <stdio.h>
main ()
{
int change;
int coins;
int remainder;
 
printf ("Please enter an amount of change from 0-99.\n");
scanf ("%d", &change);
 
if (change>=25)
{
coins = change/25;
printf ("Quarters: %d\n", coins);
remainder = change%25;
}
 
if (remainder>=10)
{
coins = remainder/10;
printf ("Dimes: %d\n", coins);
remainder = remainder%10;
}
 
if (remainder>=5)
{
coins = remainder/5;
printf ("Nickles: %d\n", coins);
remainder = remainder%5;
}
 
if (remainder>0)
{
printf ("Pennies: %d\n", remainder);
}
 
}
